{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "ECTA Documentation",
  "colors": {
    "primary": "#2d3943",
    "light": "#07C983",
    "dark": "#161c21"
  },
  "favicon": "/favicon.ico",
  "appearance": {
    "default": "dark",
    "strict": false
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/p6WDwHEU3p",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Create account",
        "href": "https://panel.ecta.games/register"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Panel",
      "href": "https://panel.ecta.games"
    }
